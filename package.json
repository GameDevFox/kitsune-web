  "scripts": {
    "build": "webpack",
    "clean": "rm -rf ./.nyc_output ./.tmp ./coverage ./dist",
    "serve": "NODE_ENV=dev webpack-dev-server",
    "test": "NODE_ENV=test webpack && nyc --reporter=html --include 'src/**/*.js' mocha --require source-map-support/register dist/spec.js",
    "test:debug": "NODE_ENV=test webpack && nyc --reporter=html --include 'src/**/*.js' mocha --inspect-brk --require source-map-support/register dist/spec.js",
    "test:watch": "NODE_ENV=test mocha-webpack --require source-map-support/register --watch --webpack-config webpack.config.js src/index.spec.js"
  }
